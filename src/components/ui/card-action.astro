---
import { cn } from "@/utils";

interface CardActionProps {
  className?: string;
}

const { className = "", ...rest } = Astro.props;
---

<div
  data-slot="card-action"
  class={cn(
    "col-start-2 row-span-2 row-start-1 self-start justify-self-end",
    className
  )}
  {...rest}
>
  <slot />
</div>

